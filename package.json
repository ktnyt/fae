{
	"name": "sfs",
	"version": "0.1.0",
	"description": "Symbol Fuzzy Search - A fast fuzzy search tool for code symbols",
	"main": "dist/index.js",
	"type": "module",
	"bin": {
		"sfs": "dist/cli.js"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsx src/cli.ts",
		"start": "node dist/cli.js",
		"check": "biome check .",
		"format": "biome format --write .",
		"lint": "biome lint .",
		"typecheck": "tsc --noEmit",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [
		"fuzzy",
		"search",
		"code",
		"symbols",
		"cli"
	],
	"author": "ktnyt",
	"license": "MIT",
	"dependencies": {
		"commander": "^12.1.0",
		"fast-glob": "^3.3.3",
		"fuse.js": "^7.1.0",
		"tree-sitter": "^0.21.1",
		"tree-sitter-javascript": "^0.23.1",
		"tree-sitter-python": "^0.23.6",
		"tree-sitter-typescript": "^0.23.2"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/node": "^24.0.0",
		"tsx": "^4.19.4",
		"typescript": "^5.8.3"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
